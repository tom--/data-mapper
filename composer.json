{
    "name": "yiisoft/data-mapper",
    "description": "Movie rating app to demonstrate data mapping in Yii 2",
    "keywords": ["data-mapper", "yii2"],
    "homepage": "https://github.com/tom--/data-mapper/tree/rating",
    "type": "project",
    "license": "ISC",
    "support": {
        "issues": "https://github.com/tom--/data-mapper/issues",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/tom--/data-mapper",
        "email": "Tom Worster <fsb@thefsb.org>"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
